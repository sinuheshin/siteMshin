<%- contentFor('css')%>

<link rel="stylesheet" href="/stylesheets/pages/page_empreendimentos.css">

<%- contentFor('breadcrumbs')%>

<h1 class="pull-left">Conheça nossos Empreendimentos</h1>
<ul class="pull-right breadcrumb">
    <li><a href="/index">Home</a></li>

    <li class="active">Empreendimentos</li>
</ul>

